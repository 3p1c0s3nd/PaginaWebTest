{{#if url}}
    <form action="/editar/{{url._id}}" method="post">
    <input type="text" name="origin" value="{{url.origin}}" placeholder="Ingresa tu URL" class="form-control my-2">
    <button type="submit" class="btn btn-warning w-100 mb-2">Editar URL</button>
    </form>

    {{else}}
    <form action="/" method="post">
        <input type="text" name="origin" placeholder="Ingresa tu URL" class="form-control my-2">
        <button type="submit" class="btn btn-primary w-100 mb-2">Agregar URL</button>
    </form>
{{/if}}

